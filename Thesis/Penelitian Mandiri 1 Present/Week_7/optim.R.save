rm(list=ls())

# masalah optimisasi
# max 7x1 + 8x2
# terhadap:
# 3x1 + 4x2 = 9
# 2x1 + x2 >= 3
# -100 <=  x1, x2 <= 100

# ================================
library(ROI)

obj_f = L_objective(c(7,8),
		    names = c("x1","x2")
		   )

A = rbind(c(3,4),c(2,1))
dir = c("==","<=")
b = c(9,3)

const = L_constraint(L = A, dir = dir, rhs = b)

bou = V_bound(li = 1:2,
	      ui = 1:2,
	      lb = c(-100,-100),
	      ub = c(100,100))

start = Sys.time()
linear_prog = OP(objective = obj_f,
 	constraints = const,
		bounds = bou)

end = Sys.time()
waktu = end - start
print("Waktu ROI: ")
print(waktu) 
